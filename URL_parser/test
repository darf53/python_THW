<script type="text/javascript">
  (function($, count, countTemplate, enableCallbacks) {
    $(document).ready(function(evt) {
      var selector = '#adv_lijst',
        countSelector = '.beheer-selection-count';

      if ($(selector).has(countSelector).length == 0) {
        return;
      }

      var selection = new TWH.beheer.selection(selector, countSelector, countTemplate, enableCallbacks);

      if (count) {
        selection.updateSelectedCount(count);
      }
    });
  })(jQuery, 0, "COUNT zoekertje(s) geselecteerd", false);
</script>

