    (function() {
      var isSafari = ($.browser.webkit && !(/chrome/.test(navigator.userAgent.toLowerCase()))),
        options = {
        };

      // The onready event is not fired on Safari if the page is retrieved from cache after the back
      // button was pressed. The pageshow event does work.
      if (isSafari) {
        $(window).on('pageshow', function(event) {
          TWH.SearchBar.initialize(options);
        });
      }
      else {
        $(document).ready(function() {
          TWH.SearchBar.initialize(options);
        });
      }
    })();
