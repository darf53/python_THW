(function($) {
  for (var i = 1; i <= 3; i++) {
    
    if ($('#lijstuitvoer').width() != $(window).width()) {
      $('.lijstuitvoer-column-container.adv-description').css({
        'width': $('.lijstuitvoer-column-container.adv-description').width() + 'px'
      });
      $(document).ready(function() {
        $('.dotdotdot').dotdotdot();
      });
      return;
    }
  }
})(jQuery);
</script>


